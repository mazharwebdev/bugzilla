<div class="container">
    <div class="row">
        <div class="col-md-6">
            <div class="panel panel-primary">
                <div class="panel-heading">
                    <h3 class="panel-title">
                        <span class="glyphicon glyphicon-bookmark"></span> Bugzilla</h3>
                </div>
                <div class="panel-body">
                    <div class="row">
                        <div class="col-xs-4 col-md-4">
                      
                          <a href="<%= projects_path %>" class="btn btn-danger btn-lg" role="button"><span class="glyphicon glyphicon-list-alt"></span>All Projects <br/></a></br></br>

                          <a href="<%= bugs_path %>" class="btn btn-danger btn-lg" role="button"><span class="glyphicon glyphicon-list-alt"></span>All Bugs <br/></a></br></br>  
                           
                        </div>

                         <div class="col-xs-4 col-md-4">
                        
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <hr>
    <div class="row">
      <%  rslt= contentful.entries(content_type: 'testFaq', include: 1).last %>
      <%= rslt.title %>  
      <h1> Multiple Images </h1>

      <% contentful.entries(content_type: 'logos', include: 1).each do |logo|
          %>
           <% logo.img1.each do |track| %>
              <%= track.title %>
              <%= image_tag track.url(width: 100, height: 100) %>
           <% end %>           
      <% end %>    
    </div>


    <h2> Testing </h2>
    <div class="row">
        <h1> FAQ </h1>
         <% contentful.entries(content_type: 'testFaq', include: 1,order: 'sys.id').each do |dep|
          %>
            <h3><%= dep.title %></h3>
                <% contentful.entries(content_type: 'post', include: 1,links_to_entry: dep.id).each do |p| %>
                        <p>Name: <%= "- -"+ p.name %></p>
                        <p>Image: <%= image_tag p.img.url(width: 300, height: 300) %></p>
                        <p><%= markdown(p.desc) %></p>
                <% end %>
          <% end %>
    </div>


</div>

